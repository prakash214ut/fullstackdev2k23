{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "build/"
      },
        {
        "src": "/products",
        "dest": "index.js"
      },
      {
        "src": "/products/(.*)",
        "dest": "build"
      }
    ]
  }
